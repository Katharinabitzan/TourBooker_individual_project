<h1> All Members </h1>

<table>
  <tr>
    <th>Full name</th>
    <th>Member ID</th>
    <th>Actions</th>
  </tr>
  <%for member in @members%>
    <tr>
      <td><%=member.name%></td>
      <td><%=member.id%></td>
      <td>
        <a href="/members/<%= member.id %>">Show</a>
        <a href="/members/<%= member.id %>/edit">Edit</a>
      </td>
    </tr>
  <%end%>

</table>
